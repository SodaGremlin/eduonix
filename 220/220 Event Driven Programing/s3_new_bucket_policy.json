{
  "Version": "2008-10-17",
  "Id": "s3-test-upload-notification",
  "Statement": [
    {
      "Sid": "example-statement-ID",
      "Effect": "Allow",
      "Principal": {
        "AWS":"*"
      },
      "Action": [
        "SNS:Publish"
      ],
      "Resource": "arn:aws:sns:us-east-1:081593210627:s3UploadTest",
      "Condition": {
        "ArnLike": { "aws:SourceArn": "arn:aws:s3:*:*:eduonix-helloworld-s3.greggharrington.com" }
      }
    }
  ]
}